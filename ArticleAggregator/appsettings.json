{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Ovveride": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WrtiteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logging\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
